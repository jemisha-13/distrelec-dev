{
  "name": "distrelecsmarteditcontainer",
  "extension": {
    "type": "container",
    "angularApp": "distrelecsmarteditContainer",
    "seEntryModule": "DistrelecsmarteditContainerModule"
  },
  "typescript": {
    "entry": "./src/index.ts",
    "dist": "./dist/index.js"
  },
  "legacy": {
    "js": [
      "./src/**/*.js"
    ],
    "ngTemplates": {
      "moduleName": "distrelecsmarteditContainerTemplates",
      "includePathInName": false,
      "files": [
        "./src/**/*.html"
      ]
    }
  }
}