{
  "name": "distrelecsmartedit",
  "extension": {
    "type": "inner",
    "angularApp": "distrelecsmartedit",
    "seEntryModule": "DistrelecsmarteditModule"
  },
  "typescript": {
    "entry": "./src/index.ts",
    "dist": "./dist/index.js"
  },
  "legacy": {
    "js": [
      "./src/**/*.js"
    ],
    "ngTemplates": {
      "moduleName": "distrelecsmarteditTemplates",
      "includePathInName": false,
      "files": [
        "./src/**/*.html"
      ]
    }
  }
}