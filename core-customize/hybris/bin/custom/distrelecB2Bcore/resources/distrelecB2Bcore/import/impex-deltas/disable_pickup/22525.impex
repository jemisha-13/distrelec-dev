#Impex for removing pickup warehouses for CH and SE webshop

# Remove pickup location for CH
UPDATE CMSSite	;uid[unique=true]	;pickupWarehouses(code)[mode=remove]
				;distrelec_CH		;"7374"					


# Remove pickup location for SE
UPDATE CMSSite	;uid[unique=true]	;pickupWarehouses(code)[mode=remove]
				;distrelec_SE		;"7641"