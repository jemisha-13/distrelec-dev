		<extension name="permissionswebservices" />
		<!--<extension name="smarteditaddon" />-->
		<extension name="addonsupport" />

		<extension name="personalizationcmsweb" />
        
		<!-- hybris patches -->
		<extension name="patchesbackoffice" />
		<extension name="distrelecpatches" />

		<extension name="azurecloud" />
		<extension name="azurecloudhotfolder" />
		<extension name="cloudcommons" />
		<extension name="cloudhotfolder" />

		<!-- OCC extensions -->
		<extension name="distrelecB2BOCC"/>
		<extension name="distrelecB2BOCCtest"/>
        <extension name="cmsoccaddon"/>
		<extension name="acceleratorwebservicesaddon"/>
		<extension name="distreleccmsfacades"/>
		<extension name="swaggerintegration"/>
