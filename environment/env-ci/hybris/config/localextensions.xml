     	<!-- <extension name="distrelecB2Bstorefront" /> -->
		<extension name="distrelecB2Bfulfilmentprocess" />
     	<extension name="distrelecB2Binitialdata" />
     	<extension name="distrelecB2Btest" />
     	<extension name="distrelecfactfindersearch" />
		<extension name="distrelecoci" />
		<!--<extension name="distrelecsmartedit" />-->
		<extension name="distrelecsmarteditwebservices" />
		<extension name="distrelectomcat" />
		<extension name="distrelecwebmedias"/>
		<extension name="distrelecwebexports"/>
		<extension name="distrelecwebpimmedias"/>

		<!-- backoffice -->
		<extension name="distrelecB2Bbackoffice" />
		<extension name="distrelecB2Bbackofficepermissions"/>
		<extension name="commerceservicesbackoffice"/>

		<!-- smartedit -->
		<!-- <extension name="smartedit" />-->
