<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- extensions.xml - hybris Multichannel Suite Note: If a file localextensions.xml is available, it is used instead of this file. this file configures which hybris 
	Platform extensions are installed, each <extension name=".."> points to an extension directory. The path can be absolute or relative. After adding an extension, call 
	'ant' to build the system. -->

<hybrisconfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="resources/schemas/extensions.xsd">
	<extensions>
		<!-- All platform extensions will be included automatically. -->
		
		<path dir="${HYBRIS_BIN_DIR}" depth="4" autoload="false"/>
		
		<!-- ext-commerce -->
		<extension name="b2bapprovalprocess" />
		<extension name="b2bcommerce" />
		<extension name="basecommerce" />
		
		
		<extension name="commercefacades" />
		<extension name="commerceservices" />
		<extension name="customerreview" />
		<extension name="payment" />
		
		<extension name="wishlist" />


		<!-- ext-content -->
		<extension name="cms2" />
		<extension name="cms2lib" />


		<!-- ext-accelerator -->
		<extension name="acceleratorcms" />
		<extension name="acceleratorfacades" />
		<extension name="acceleratorservices" />
		<extension name="b2bacceleratorfacades" />
		<extension name="b2bacceleratorservices" />
		

		<!-- **************************************************************** -->
		<!-- *** Project specific extensions *** -->
		<!-- **************************************************************** -->
		<extension name="namtoolbox" />
		<extension name="nami18n" />
		<extension name="namwebservice" />
		<!-- Expressway Extensions -->
		<extension name="sapymktcommon" />
		<extension name="sapymktsync" />

 		<!-- ext-backoffice -->
		<extension name="b2bcommercebackoffice"/>
