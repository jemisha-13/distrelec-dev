  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot --optimization=true --vendor-chunk=false",
    "build": "ng build --aot --build-optimizer --vendor-chunk=false --stats-json",
    "build:stats": "ng build --stats-json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e": "ng e2e",
    "dev": "ng serve --ssl --watch --configuration development",
    "dev:debug": "ng serve --watch --configuration development",
    "dev:ssr": "NODE_OPTIONS=--max_old_space_size=8192 NODE_TLS_REJECT_UNAUTHORIZED=0 ng run distrelecStore:serve-ssr:development",
    "prod:ssr": "NODE_TLS_REJECT_UNAUTHORIZED=0 ng run distrelecStore:serve-ssr:production",
    "serve:ssr": "node dist/distrelecStore/server/main.js --configuration production --aot --optimization=true --vendor-chunk=false",
    "build:ssr": "ng build && ng run distrelecStore:server",
    "prerender": "ng run distrelecStore:prerender",
    "dev:ssr:dev": "cross-env NODE_OPTIONS=--max_old_space_size=8192 NODE_TLS_REJECT_UNAUTHORIZED=0 ng run distrelecStore:serve-ssr:development",
    "analyse": "webpack-bundle-analyzer dist/distrelecStore/browser/stats.json",
    "lint": "ng lint",
    "docs:json": "compodoc -p ./tsconfig.json ./src/design-system -e json -d .",
    "storybook": "ng run distrelecStore:storybook",
    "build-storybook": "ng run distrelecStore:build-storybook",
    "chromatic": "npx chromatic --project-token=chpt_43ce24eefceaad8 -o ./storybook-build --zip",
    "chromatic:test": "npx chromatic --project-token=chpt_43ce24eefceaad8 -o ./storybook-build --zip --dry-run",
    "generate-icon-files": "cd src/app/spartacus/assets/icons && node ./convert.js && npx prettier . --write",
    "prepare": "cd ../.. && husky js-storefront/distrelecStore/.husky"
  },
